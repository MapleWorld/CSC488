{
    var a : Integer
    var b : Boolean

    %%%%% function with parameters
    function A(m : Integer, n : Integer) : Integer {
        if m = 0 then
            return with n + 1
        else if n = 0 then
            return with A(m - 1, 1)
        else 
            return with A(m - 1, A(m, n - 1))

    }

    var m, n : Integer
    write "enter two numbers: ", newline
    read m, n
    write "A(",m,",",n,") = ", A(m, n)

}
